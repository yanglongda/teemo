<view class="content">
  <view class="baseInfo">
    <view class="line">
      <view class="text-name">学籍号
      </view>
      <view class="text-value">{{userBaseInfo.studentId}}
      </view>
    </view>
    <view class="line">
      <view class="text-name">姓名
      </view>
      <view class="text-value">{{userBaseInfo.name}}
      </view>
    </view>
    <view class="line">
      <view class="text-name">性别
      </view>
      <view class="text-value">{{userBaseInfo.sex}}
      </view>
    </view>
    <view class="line">
      <view class="text-name">出生年月
      </view>
      <view class="text-value">{{userBaseInfo.birthDay}}
      </view>
    </view>
    <view class="line">
      <view class="text-name">民族
      </view>
      <view class="text-value">{{userBaseInfo.nation}}
      </view>
    </view>
    <view class="line">
      <view class="text-name">政治面貌
      </view>
      <view class="text-value">{{userBaseInfo.political}}
      </view>
    </view>
    <view class="line">
      <view class="text-name">联系电话
      </view>
      <view class="text-value">{{userBaseInfo.phone}}
      </view>
    </view>
    <view class="line">
      <view class="text-name">就读状态
      </view>
      <view class="text-value">{{userBaseInfo.status}}
      </view>
    </view>
