      <view class="text-value">{{userBaseInfo.level}}
      </view>
    </view>
    <view class="line">
      <view class="text-name">年级
      </view>
      <view class="text-value">{{userBaseInfo.grade}}
      </view>
    </view>
    <view class="line">
      <view class="text-name">班级名称
      </view>
      <view class="text-value">{{userBaseInfo.className}}
      </view>
    </view>
    <view class="line">
      <view class="text-name">教育阶段
      </view>
      <view class="text-value">{{userBaseInfo.education}}
      </view>
    </view>
    <view class="line">
      <view class="text-name">学号
      </view>
      <view class="text-value">{{userBaseInfo.studentNo}}
      </view>
    </view>
    <view class="line">
      <view class="text-name">专业名称
      </view>
      <view class="text-value">{{userBaseInfo.professional}}
      </view>
    </view>
  </view>
